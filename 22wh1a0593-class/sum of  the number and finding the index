class Sum{
    public static void main(String[] args) {
        int nums[] ={1,2,3,4,6,7,8};
        int target=100;
        int i;
        for(i=0;i<nums.length-1;i++){
            for(int j=i+1;j<nums.length;j++){
            if(nums[i]+nums[j]==target){
                System.out.print("["+i+","+j+"]");
                break;
            }
            
            }
        }
    } 
}
