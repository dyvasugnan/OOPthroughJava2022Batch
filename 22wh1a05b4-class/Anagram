import java.io.*;
import java.util.*;
class Anagram{
    public static void main(String[] args) {
        String s1="Top";
        String s2="Pot";
        char arr[]=s1.toLowerCase().toCharArray();
        char arr1[]=s2.toLowerCase().toCharArray();
        Arrays.sort(arr);
        Arrays.sort(arr1);
        if(Arrays.equals(arr,arr1)==true)
        {
            System.out.print("anagram");
        }
        else{
            System.out.print("not anagram");
        }
    }
}
