import java.util.*;
class palindrome {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        String s1=sc.next();
        int i;
        String s2="";
        for ( i = (s1.length() - 1); i >=0; --i) {
          s2 = s2+ s1.charAt(i);
        }
        if(s1.equals(s2))
        {
          System.out.print("palindrome");
        }
        else
        {
            System.out.print("not palindrome");
        }
    }
}
